 "font_size": 14,
 "html": "<style>\n        .print-format {\n              padding: 0px;\n              margin-left: 40px;\n            }\n          \n            @media screen {\n              .print-format {\n                padding: 0in;\n                \n              }\n            }\n    .qr {\n        margin-left: 168px;\n    \n        margin-top: -22px;\n\n       \n    }\n    .barcode {\n       margin-top: -14px;\n       margin-left: -125px;\n\n        \n    }\n    .name {\n       margin-top: 28px;\n       margin-left: -121px;\n       margin-bottom: 13px;\n       font-size: 9px;\n\n        \n    }\n    .batch {\n        margin-left: -41px;\n        margin-top: 1px;\n        margin-bottom: 1px;\n        font-size: 8px;\n    }\n    .price {\n        font-size: 9px;\n        margin-top: -1px;\n        margin-left: -125px;\n    }\n    .date {\n        font-size: 9px;\n        margin-top: -12px;\n        margin-left: -65px;\n    }\n    .mrp {\n        font-size: 9px;\n        margin-top: -13px;\n        margin-left: 4px;\n    }\n</style>\n\n<div class=\"qr\">\n    {% for item in doc.items %}\n        {% set barcode = frappe.db.get_value(\"Item Barcode\", {\"parent\": item.item_code}, \"barcode\") %}\n        {% set settings = frappe.db.get_value(\"Calicut Textiles Settings\",None, \"price_listmrp\") %}\n        {% set item_price = frappe.db.get_value(\"Item Price\", {\"item_code\": item.item_code, \"price_list\": \"Standard Selling\"}, \"custom_rate_code\") %}\n        {% set item_price_mrp = frappe.db.get_value(\"Item Price\", {\"item_code\": item.item_code, \"price_list\": settings}, \"custom_rate_code\") %}\n        \n        {% if item.serial_no %}\n            {% for serial in item.serial_no.split('\\n') %}\n                {% set qty_int = 1 %}\n                \n                {% if item.batch_no %}\n                    <div class=\"batch\">\n                        <b>Batch No: {{ item.batch_no }}  [ {{ item.qty }} ]</b>\n                    </div>\n                {% endif %}\n                <div class=\"name\">\n                    <b >{{item.item_name }}</b>\n                </div>\n                \n                <div class=\"barcode\">\n                    <img src=\"https://barcode.tec-it.com/barcode.ashx?data={{ item.custom_batch }}&code=Code128&translate-esc\" width='150' height='60'>\n                </div>\n                \n                <div class=\"barcode\">\n                    <b>{{ item_price }}</b>\n                </div>\n            {% endfor %}\n        {% else %}\n            {% set qty_int = item.qty | round | int %}\n            {% for i in range(qty_int) %}\n                \n                {% if item.batch_no %}\n                    <div class=\"batch\">\n                        <b>Batch No: {{ item.batch_no }}  [ {{ item.qty }} ]</b>\n                    </div>\n                {% endif %}\n                    <div class=\"name\">\n                    <b>{{item.item_name }}</b>\n                </div>\n                \n                <div class=\"barcode\">\n                    <img src=\"https://barcode.tec-it.com/barcode.ashx?data={{ item.custom_batch }}&code=Code128&translate-esc\" width='150' height='60'>\n                                    </div>\n                     <div class=\"price\">\n                    <b>{{ item_price }}</b>\n                </div>\n                <div class=\"date\">\n                    <b>{{ doc.custom_date_code }}</b>\n                </div>\n                <div class=\"mrp\">\n                    <b>{{ item_price_mrp }}</b>\n                </div>\n\n                \n               \n            {% endfor %}\n        {% endif %}\n    {% endfor %}\n</div>\n",
